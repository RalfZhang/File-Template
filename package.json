{
    "name": "filetemplate",
    "displayName": "File Template",
    "description": "An extension for creating file from templates automatically.",
    "version": "2.0.4",
    "publisher": "RalfZhang",
    "license": "MIT",
    "icon": "logo.png",
    "keywords": [
        "file",
        "template",
        "tmpl",
        "snippet"
    ],
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.tmpl",
        "onCommand:extension.tmpljavascript",
        "onCommand:extension.tmplhtml",
        "onCommand:extension.tmplcss",
        "onCommand:extension.tmplphp",
        "onCommand:extension.tmplpython",
        "onCommand:extension.tmplruby",
        "onCommand:extension.tmplxml",
        "onCommand:extension.tmplvue"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.tmpl",
                "title": "Tmpl: Create Template"
            },
            {
                "command": "extension.tmpljavascript",
                "title": "Tmpl js: Create JavaScript Template"
            },
            {
                "command": "extension.tmplhtml",
                "title": "Tmpl html: Create HTML Template"
            },
            {
                "command": "extension.tmplcss",
                "title": "Tmpl css: Create CSS Template"
            },
            {
                "command": "extension.tmplphp",
                "title": "Tmpl php: Create PHP Template"
            },
            {
                "command": "extension.tmplpython",
                "title": "Tmpl py: Create Python Template"
            },
            {
                "command": "extension.tmplruby",
                "title": "Tmpl rb: Create Ruby Template"
            },
            {
                "command": "extension.tmplxml",
                "title": "Tmpl xml: Create XML Template"
            },
            {
                "command": "extension.tmplvue",
                "title": "Tmpl vue: Create Vue Template"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
